---
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title }}",
      "desc"     : "{{ post.description }}",
      "category" : "{{ post.category }}",
      "english"  : "{{ post.english }}",
      "tags"     : [{% for tag in post.tags %}"{{ tag }}",{% endfor %} null],
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date | date_to_string }}"
    },
  {% endfor %}
  null
]